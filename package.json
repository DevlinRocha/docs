{
  "name": "prisma-docs",
  "description": "Prisma.io docs",
  "version": "3.0.0",
  "author": "Nilufar Bava <bava@prisma.io>",
  "dependencies": {
    "@docusaurus/core": "^3.1.1",
    "@docusaurus/preset-classic": "^3.1.1",
    "@mdx-js/loader": "1.6.22",
    "@mdx-js/mdx": "1.6.22",
    "@mdx-js/react": "^3.0.0",
    "@philpl/buble": "0.19.7",
    "@react-aria/overlays": "^3.20.0",
    "@react-aria/tooltip": "^3.7.0",
    "@react-aria/utils": "^3.23.0",
    "@react-stately/tooltip": "^3.4.6",
    "algoliasearch": "4.14.2",
    "babel-plugin-styled-components": "2.0.7",
    "clsx": "^2.0.0",
    "is-absolute-url": "^4.0.1",
    "normalize.css": "^8.0.1",
    "parse-numeric-range": "1.3.0",
    "path-browserify": "^1.0.1",
    "prism-react-renderer": "^2.3.0",
    "prop-types": "15.7.2",
    "qs": "^6.11.2",
    "react": "^18.0.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.0.0",
    "react-feather": "^2.0.10",
    "react-instantsearch-dom": "^6.40.4",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^6.21.3",
    "react-sticky-box": "^2.0.5",
    "react-toastify": "9.0.8",
    "styled-components": "^6.1.8",
    "write": "2.0.0"
  },
  "engines": {
    "node": ">=18.0"
  },
  "keywords": [
    "mdx",
    "typescript",
    "prismaio",
    "howtodata",
    "databases",
    "styled-components"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "clean": "docusaurus clear",
    "build": "docusaurus build",
    "dev": "docusaurus start",
    "serve": "docusaurus serve",
    "format": "prettier . --write --check",
    "install-algolia": "cd ./plugins/gatsby-algolia-indexer && npm install",
    "postinstall": "husky install",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "pages:dev": "NODE_OPTIONS=\"--openssl-legacy-provider\" wrangler pages dev --compatibility-date=2024-01-17 --proxy 3000 -- npm run start",
    "pages:deploy": "NODE_OPTIONS=\"--openssl-legacy-provider\" npm run build && wrangler pages deploy ./build"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.1.1",
    "@docusaurus/tsconfig": "^3.1.1",
    "@docusaurus/types": "^3.1.1",
    "@types/path-browserify": "^1.0.2",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-instantsearch-dom": "^6.12.7",
    "prettier-plugin-prisma": "^5.0.0",
    "typescript": "~5.2.2",
    "wrangler": "^3.24.0"
  },
  "lint-staged": {
    "*.*": "prettier --write --ignore-unknown"
  },
  "percy": {
    "version": 2,
    "snapshot": {
      "enableJavaScript": true,
      "widths": [
        1280
      ]
    },
    "discovery": {
      "launch-options": {
        "timeout": 100000
      }
    }
  },
  "resolutions": {
    "loader-utils": "^2.0.1"
  }
}
